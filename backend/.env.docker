# Docker Environment Variables
# Copy this to .env and update with your values

# Database Configuration
POSTGRES_DB=smd_hardware
POSTGRES_USER=smd_user
POSTGRES_PASSWORD=your_secure_database_password_here

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_key_minimum_32_characters_long
JWT_REFRESH_SECRET=your_super_secure_refresh_secret_key_minimum_32_characters_long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# CORS Configuration (Frontend URLs)
CORS_ORIGIN=https://www.catalogquienquillerie.sqb-tunisie.com,https://www.admin-dashboard.sqb-tunisie.com

# Upload Configuration
MAX_FILE_SIZE=5242880

# Admin Configuration
ADMIN_EMAIL=admin@sqb-tunisie.com
ADMIN_PASSWORD=your_secure_admin_password_here
